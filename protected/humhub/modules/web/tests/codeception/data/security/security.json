{
  "headers": {
    "Strict-Transport-Security": "max-age=31536000",
    "X-XSS-Protection": "1",
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "deny"
  },
  "csp": {
    "nonce": true,
    "report-only": false,
    "default-src": {
      "self": true
    },
    "script-src": {
      "allow": [],
      "self": true,
      "unsafe-inline": true,
      "unsafe-eval": false,
      "report-sample": true
    },
    "style-src": {
      "self": true,
      "unsafe-inline": true
    },
    "upgrade-insecure-requests": true
  }
}