            $details.stop().slideToggle('fast', function () {
                $details.remove();
            });

            $showMore.find('i').attr('class', 'fa fa-angle-up');
        } else {
            $details = $(string.template(module.template.errorBlock, {msg: getErrorMessage(error)}));
